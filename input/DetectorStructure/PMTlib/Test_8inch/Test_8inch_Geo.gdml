	</skinsurface> 
	<volume name="BodyLog">
		<materialref ref="PMTShell"/>
		<solidref ref="thepolycone"/>
	</volume>

	<volume name="_8inPMT">
		<materialref ref="PMTShell"/>
		<solidref ref="_8PMT"/>
